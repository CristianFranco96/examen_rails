<h1>Sucursales Actuales</h1>
<div class='container'>
    <table class='table'>
      <thead>
        <tr>
          <th>Sucursal</th>
          <th>Dirreccion</th>
          <th>CP</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @sucursales.each do |sucursal| %>
          <tr>
            <td><%= sucursal.sucursal_name %></td>
            <td><%= sucursal.sucursal_add %></td>            
            <td><%= sucursal.zip_code %></td>
            <td><a href='/sucursals/<%= sucursal.id%>/edit'>Editar</a></td>
          </tr>
        <% end %>
      </tbody>    
    </table>
    
</div>